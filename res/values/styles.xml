<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="buttom_top" parent="android:Widget.Button">
        <item name="android:layout_width">66dp</item>
        <item name="android:layout_height">31dp</item>
        <item name="android:textColor">@drawable/radio_buttom_text_color</item>
        <item name="android:button">@null</item>
    </style>

    <style name="Dialog_Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>
    </style>

    <style name="style_text_navigation" parent="@style/width_wrap_height_wrap">
        <item name="android:textColor">#989898</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="style_image_navigation">
        <item name="android:paddingTop">5dp</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="style_cell_navigation" parent="@style/width_fill_height_fill">
        <item name="android:background">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="style_top_base">
        <item name="android:background">#66cccc</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="style_top_text" parent="@style/width_wrap_height_wrap">
        <item name="android:background">@null</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="radio_button_style_green">
        <item name="android:background">@null</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="home_buttons">
        <item name="android:layout_width">95dp</item>
        <item name="android:layout_height">110dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="image_button_forword">
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:layout_gravity">right|center_vertical</item>
        <item name="android:layout_width">8dp</item>
        <item name="android:layout_height">12dp</item>
        <item name="android:background">@drawable/personal_change</item>
    </style>

    <style name="under_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/gray_d</item>
    </style>

    <style name="personal_text_view" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="personal_btn_item">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="text_view_right_gray" parent="@style/width_wrap_height_wrap">
        <item name="android:gravity">center</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:textColor">@color/gray_6</item>
    </style>

    <style name="linearlayout_line_item">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="textview_left_black" parent="@style/personal_text_view">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">15dp</item>
    </style>

    <style name="login_regist_input" parent="@style/width_fill_height_wrap">
        <item name="android:background">@null</item>
        <item name="android:ems">10</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="input_item">
        <item name="android:background">#1D8F83</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">55dp</item>
    </style>

    <style name="ic_hint">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_marginLeft">15dp</item>
    </style>

    <style name="regist_editor_style" parent="@style/width_fill_height_wrap">
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#000</item>
        <item name="android:textColorHint">#97989E</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="btn_green_style" parent="android:Widget.Button">
        <item name="android:background">@drawable/green_btn</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">43dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="regist_input_style" parent="@style/width_fill_height_wrap">
        <item name="android:background">@null</item>
    </style>
    <style name="pretty_school_mate_text">
        <item name="android:gravity">center</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="save_wdjy_buttom_btn" parent="android:Widget.ImageButton">
        <item name="android:clickable">false</item>
         <item name="android:layout_height">22dp</item>
        <item name="android:layout_width">28dp</item>
    </style>
    <style name="save_wdjy_buttom_btn_text" parent="@style/width_wrap_height_wrap">
        <item name="android:textColor">#FF524F4F</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="save_wdjy_btn_item">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:descendantFocusability">beforeDescendants</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>
    
    
    <style name="home_title_text_style" parent="@style/width_fill_height_wrap">
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:lineSpacingMultiplier">1.1</item>
        <item name="android:maxLines">2</item>
        <item name="android:textColor">#0e0e0d</item>
        <item name="android:textSize">22sp</item>
    </style>
    
    <style name="width_fill_height_wrap">
          <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="width_fill_height_fill">
          <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="width_wrap_height_wrap">
          <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="width_wrap_height_fill">
          <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="home_content_text_style" parent="android:Widget.TextView">
        <item name="android:lineSpacingExtra">1dp</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:textColor">#898985</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="text_frame_radius_style" parent="@style/width_wrap_height_wrap">
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="list_view_nofading" parent="@style/width_fill_height_fill">
        <item name="android:cacheColorHint">#0000</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fadingEdgeLength">0dp</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="famous_says_info_style" parent="@style/width_wrap_height_wrap">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">#6C6B66</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="more_btn_style" parent="android:Widget.TextView">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:visibility">gone</item>
    </style>
    <style name="gradeline" parent="@style/width_wrap_height_wrap">
        <item name="android:gravity">center</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="gray_e_line" >
         <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/gray_e</item>
        <item name="android:layout_marginBottom">20dp</item>
        <item name="android:layout_marginTop">20dp</item>
    </style>
</resources>